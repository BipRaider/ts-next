    "prepare": "husky install"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/classnames": "^2.3.1",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.5.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.5",
    "sass": "^1.59.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.2.4",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/sass": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
