{
  "name": "ts-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:fix": "eslint src/**/*.{js,jsx,ts,tsx} --fix",
    "docker:up": "docker-compose up -d",
    "docker:dev": "docker-compose rm -sf app-dev && docker-compose -f docker-compose.yaml up -d app-dev",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
