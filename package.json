{
  "name": "ts-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "lint": "next lint --fix",
    "eslint": "eslint src/**/*.{ts,tsx} --fix",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "docker:up": "docker-compose up -d",
